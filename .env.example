# Telegram API credentials from my.telegram.org
API_ID=your_api_id
API_HASH=your_api_hash
PHONE_NUMBER=your_phone_number # (e.g., +998123456789)

# Your display name shown in Telegram first_name
NAME=your_name # e.g., Buzruk

# Geographic coordinates and timezone for location-based data (e.g., prayer times, weather)
LAT=your_latitude           # Latitude of your location (e.g., 41.2995 for Tashkent)
LON=your_longitude          # Longitude of your location (e.g., 69.2401 for Tashkent)
TIMEZONE=your_timezone      # IANA timezone name (e.g., Asia/Tashkent)
CITY=your_city              # Name of your city (e.g., Tashkent)

# API key for fetching weather data (from OpenWeatherMap)
WEATHER_API_KEY=your_weather_api_key